<views:BaseWindow x:Class="CatsTaskProject.Views.MainWindow"
        xmlns:reactiveui="https://www.reactiveui.net/"
        xmlns:viewModels="clr-namespace:CatsTaskProject.ViewModels"
        xmlns:views="clr-namespace:CatsTaskProject.Views"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CatsTaskProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    
    <Window.DataContext>
        <viewModels:MainWindowViewModel/>
    </Window.DataContext>
    
    <Grid>
        <Button Command="{Binding GetCatBreedsCommand}" Width="250" Height="50">GetBreeds</Button>
        <Label Content="{Binding}" Height="100" Width="100" Margin="562,167,138,167" Background="yellow"/>
    </Grid>
</views:BaseWindow>
