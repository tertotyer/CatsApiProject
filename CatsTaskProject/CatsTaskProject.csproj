<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>disable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Resources\paw.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\leftArrow.png" />
    <None Remove="Resources\loupe.png" />
    <None Remove="Resources\paw.ico" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\paw.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="ReactiveUI" Version="20.1.63" />
    <PackageReference Include="System.Drawing.Common" Version="9.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\LeftArrow.png" />
    <Resource Include="Resources\Loupe.png" />
    <Resource Include="Resources\paw.ico" />
  </ItemGroup>

</Project>
